---
const res = await fetch('https://api.thecatapi.com/v1/images/search?limit=3&mime_types=gif')
const data = await res.json()
console.log(data) 

--- 


<section class=" grid grid-cols-3 object-cover"> 
{
    data.map((cat) => {
        return (
            <div class=" ">
                <img class="transition-all duration-500 overflow-auto" src={cat.url} alt="cat" width="300" height="200" />
            </div>
        )
    })
}
</section>